<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	
	<class name="org.myls.scan.SimpleScanParameters">
		<map-to xml="SimpleScanParameters" />
		<field name="name" type="string">
			<bind-xml name="name" node="element" />
		</field>
		<field name="start" type="double">
			<bind-xml name="start" node="element" />
		</field>
		<field name="end" type="double">
			<bind-xml name="end" node="element" />
		</field>
		<field name="seconds" type="integer">
			<bind-xml name="seconds" node="element" />
		</field>
	</class>
</mapping>
	