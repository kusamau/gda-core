#!/bin/bash
`dirname $0`/gda_commands logserver --restart $@
`dirname $0`/gda_commands nameserver --restart $@
`dirname $0`/gda_commands eventserver --restart $@
# default debug and profile ports: --debugport=8000, --profileport=8851
JAVA_OPTS="-Xms128m -Xmx1024m -Dgda.mode=test" `dirname $0`/gda_commands objectserver --debug --restart $@