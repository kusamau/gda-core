<?xml version="1.0" encoding="UTF-8"?>
<mapping>
    <class name="gda.factory.Findable" auto-complete="true" verify-constructable="false"/>
    <class name="gda.factory.ObjectFactory" auto-complete="true">
        <map-to xml="ObjectFactory" />
        <field name="findables" type="gda.factory.Findable" collection="collection"
            container="false">
            <bind-xml name="Findables" node="element" />
        </field>
    </class>    
    <class name="gda.factory.XmlObjectCreatorTest" identity="name">
        <map-to xml="ObjectCreatorTest" />
        <field name="name" type="string">
            <bind-xml name="name" node="element" />
        </field>        
    </class>
</mapping>
