<?xml version="1.0" ?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!--
		N.B. this stuff breaks Xerlin
		targetNamespace="http://www.dl.ac.uk/GDASchema"
		xmlns="http://www.dl.ac.uk/GDASchema">
	-->
	<xsd:annotation>
		<xsd:documentation>
			simple restriction types
	</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="directoryType">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="secondsAndMinutesType">
		<xsd:restriction base="xsd:nonNegativeInteger">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="59"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="hourType">
		<xsd:restriction base="xsd:nonNegativeInteger">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="23"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="daysType">
		<xsd:restriction base="xsd:nonNegativeInteger">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="31"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="fileTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BINARY"/>
			<xsd:enumeration value="CHAR"/>
			<xsd:enumeration value="SRB"/>
			<xsd:enumeration value="STORAGED"/>
			<xsd:enumeration value="STORAGEDICAT"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="timeType">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="seconds"/>
			<xsd:element minOccurs="0" ref="minutes"/>
			<xsd:element minOccurs="0" ref="hours"/>
			<xsd:element minOccurs="0" ref="days"/>
			<xsd:element minOccurs="0" ref="time"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
			types used by elements in ObjectFactory - but may not be directly
			instantiated
    </xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="DeviceBase">
		<xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="1" ref="name"/>
			<xsd:element maxOccurs="1" minOccurs="0" ref="protectionLevel"/>
			<xsd:element maxOccurs="1" minOccurs="0" ref="local"/>
			<xsd:element maxOccurs="1" minOccurs="0" ref="configureAtStartup"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
			messaging / logging service classes
    </xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="MessagePanel">
		<xsd:sequence>
			<xsd:element ref="name"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LogPanel">
		<xsd:sequence>
			<xsd:element ref="name"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DebugPanel">
		<xsd:sequence>
			<xsd:element ref="name"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
			Generic Data Array Manager
    </xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="GenericDataManagerType">
		<xsd:sequence>
			<xsd:element ref="name"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
			metadata classes
    </xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="FindableHashtable">
		<xsd:sequence>
			<xsd:element ref="name"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="metadataType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="icat"/>
			<xsd:enumeration value="scannable"/>
			<xsd:enumeration value="jython"/>
			<xsd:enumeration value="property"/>
			<xsd:enumeration value="stored"/>
			<xsd:enumeration value="epics"/>
			<xsd:enumeration value="date"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="Icat">
		<xsd:sequence>
			<xsd:element ref="name"/>
			<xsd:element ref="url"/>
			<xsd:element ref="user"/>
			<xsd:element ref="password"/>
			<xsd:element minOccurs="0" ref="shiftTolerance"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BeamlineConfigurationManager">
		<xsd:sequence>
			<xsd:element ref="name"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="NexusMetadataEntry">
		<xsd:sequence>
			<xsd:element ref="name"/>
			<xsd:element minOccurs="0" ref="accessName"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="NexusMetadataReader">
		<xsd:sequence>
			<xsd:element ref="name"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="NexusMetadataEntry"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
			Adc
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="DummyAdc">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AdcMonitor">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element ref="adcName"/>
					<xsd:element ref="channel"/>
					<xsd:element ref="pollTime"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="Keithley">
		<xsd:sequence>
			<xsd:element ref="name"/>
			<xsd:element ref="gpibInterfaceName"/>
			<xsd:element ref="deviceName"/>
			<xsd:element ref="timeout"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DummyKeithley">
		<xsd:sequence>
			<xsd:element ref="name"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
			Attenuators
		</xsd:documentation>
	</xsd:annotation>
	
	<xsd:annotation>
		<xsd:documentation>
			Data Loggers
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="HPDataLogger">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element ref="serialDeviceName"/>
					<xsd:element ref="polltime"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DummyDataLogger">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element ref="serialDeviceName"/>
					<xsd:element ref="polltime"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:annotation>
		<xsd:documentation>
			Scannables
		</xsd:documentation>
	</xsd:annotation>

	<xsd:complexType name="ScannableBase">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="0" ref="level"/>
					<xsd:choice maxOccurs="unbounded" minOccurs="0">
						<xsd:element ref="inputName"/>
						<xsd:element ref="extraName"/>
						<xsd:element ref="outputFormat"/>
					</xsd:choice>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="ScannableMotionBase">
		<xsd:complexContent>
			<xsd:extension base="ScannableBase">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="tolerance"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="numberTries"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ScannableMotionUnitsBase">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionBase">
				<xsd:sequence>
					<!-- xsd:element maxOccurs="unbounded" minOccurs="0" ref="acceptableUnits" /-->
					<xsd:element maxOccurs="1" minOccurs="0" ref="hardwareUnitString"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="initialUserUnits"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="userUnits"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ScannableMotor">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionUnitsBase">
				<xsd:sequence>
					<xsd:element ref="motorname"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="motorunits"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="offset"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="scalingFactor"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="lowerlimit"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="upperlimit"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="level"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="outputformat"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CheckedScannableMotor">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotor">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" ref="scannableName"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="scannableTolerance"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="ConvertorScannable">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionUnitsBase">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" ref="convertor"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="scannable"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="CoupledScannable">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionUnitsBase">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="1" ref="scannable"/>
					<!-- all the possible types of functions -->
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="IdentityFunction"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="LinearFunction"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="InterpolationFunction"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="SimpleTrigFunction"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TwoJawSlitPosition">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionUnitsBase">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" ref="firstJaw"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="secondJaw"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="TwoJawSlitGap">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionUnitsBase">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" ref="firstJaw"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="secondJaw"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="xmlparametersfile"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="MonoScannable">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionBase">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" ref="motor"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="motorUnit"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="userUnit"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="crystalType"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="twoD"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="SineDriveAngularScannable">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionBase">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" ref="motor"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="angleOffset"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="armLength"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="SineDriveMonoScannable">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionBase">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" ref="motor"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="correctionFactor"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="gratingDensity"/>
					<xsd:element maxOccurs="1" minOccurs="1" ref="sineArmLength"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	

	<xsd:complexType name="DummyScannable">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionBase">
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="DummyUnitsScannable">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionUnitsBase">
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DummyPersistentScannable">
		<xsd:complexContent>
			<xsd:extension base="DummyScannable">
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="DummyMultiElementScannable">
		<xsd:complexContent>
			<xsd:extension base="ScannableBase">
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="MecartexAxis">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionUnitsBase">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" ref="axis"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="host"/>
					<xsd:element maxOccurs="1" minOccurs="0" ref="port"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="MecartexScannable">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionUnitsBase">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="0" ref="host" />
					<xsd:element maxOccurs="1" minOccurs="0" ref="port" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="ActorMicroglideAxis">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionUnitsBase">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="1" ref="axis"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

		<xsd:complexType name="ActorMicroglide">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionUnitsBase">
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	

	<xsd:annotation>
		<xsd:documentation>
			Detectors / Counter Timers
	</xsd:documentation>
	</xsd:annotation>

	<xsd:complexType name="DetectorBase">
		<xsd:complexContent>
			<xsd:extension base="ScannableBase"/>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="DummyDetector">
		<xsd:complexContent>
			<xsd:extension base="ScannableBase"/>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="CounterTimerBase">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="channelLabel"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="DummyCounterTimer">
		<xsd:complexContent>
			<xsd:extension base="CounterTimerBase">
				<xsd:sequence>
					<xsd:element minOccurs="0" ref="totalChans"/>
					<xsd:element minOccurs="0" ref="useGaussian"/>
					<xsd:element minOccurs="0" ref="gaussianWidth"/>
					<xsd:element minOccurs="0" ref="gaussianHeight"/>
					<xsd:element minOccurs="0" ref="noiseLevel"/>
					<xsd:element minOccurs="0" ref="timeChannelRequired"/>
					<xsd:element minOccurs="0" ref="slave"/>
					<xsd:element minOccurs="0" ref="tfgName"/>
					<xsd:element minOccurs="0" ref="usingMoveable"/>
					<xsd:element minOccurs="0" ref="moveableName"/>
					<xsd:element minOccurs="0" ref="dataDecimalPlaces"/>
					<xsd:element minOccurs="0" ref="stepsPerUnit"/>
					<xsd:element minOccurs="0" ref="readoutFileName"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:simpleType name="imageFormats">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="png"/>
			<xsd:enumeration value="jpg"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:complexType name="DummyImageCreator">
		<xsd:complexContent>
			<xsd:extension base="DetectorBase">
				<xsd:sequence>
					<xsd:element ref="width"/>
					<xsd:element ref="height"/>
					<xsd:element ref="format"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="NI6602">
		<xsd:complexContent>
			<xsd:extension base="CounterTimerBase">
				<xsd:sequence>
					<xsd:element ref="totalChans"/>
					<xsd:element ref="deviceNumber"/>
					<xsd:element ref="timingChannel"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:annotation>
		<xsd:documentation>
			Detectors / Controller Based
        </xsd:documentation>
	</xsd:annotation>
	
	<xsd:complexType name="CoboldTDC">
		<xsd:complexContent>
			<xsd:extension base="DetectorBase">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="channelLabel"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="DummyCoboldTDC">
		<xsd:complexContent>
			<xsd:extension base="DetectorBase">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="channelLabel"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:annotation>
		<xsd:documentation>
			Detectors / Analysers
        </xsd:documentation>
	</xsd:annotation>
	
	<xsd:complexType name="AnalyserBase">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase"/>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="IPPserver">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element ref="outputFolderRoot"/>
					<xsd:element ref="fileNameRoot"/>
					<xsd:element ref="fileFormat"/>
					<xsd:element ref="collectionTime"/>
					<xsd:element ref="host"/>
					<xsd:element ref="port"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SesoXbpmDetector">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element ref="host"/>
					<xsd:element ref="port"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:annotation>
		<xsd:documentation>
			EnumPositioners
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="DummyEnumPositioner">
		<xsd:complexContent>
			<xsd:extension base="ScannableMotionBase">
				<xsd:sequence>
					<xsd:element maxOccurs="12" ref="position"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DummyValve">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase"/>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:annotation>
		<xsd:documentation>
			FilterArrays
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="DummyFilterArray">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase"/>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:annotation>
		<xsd:documentation>
			Gpib
    </xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="GpibNiWin">
		<xsd:sequence>
			<xsd:element ref="name"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DummyGpib">
		<xsd:sequence>
			<xsd:element ref="name"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
			Memory
    </xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="Dimension">
		<xsd:sequence>
			<xsd:element ref="width"/>
			<xsd:element ref="height"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Gdhist">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element ref="daServerName"/>
					<xsd:element ref="width"/>
					<xsd:element ref="height"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="supportedDimension"/>
					<xsd:element ref="openCommand"/>
					<xsd:element minOccurs="0" ref="startupScript"/>
					<xsd:element minOccurs="0" ref="sizeCommand"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="Scaler">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element ref="daServerName"/>
					<xsd:element ref="width"/>
					<xsd:element ref="height"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" ref="supportedDimension"/>
					<xsd:element ref="openCommand"/>
					<xsd:element minOccurs="0" ref="startupScript"/>
					<xsd:element minOccurs="0" ref="sizeCommand"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EncoderCounter">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element ref="daServerName"/>
					<xsd:element ref="Dimension"/>
					<xsd:element ref="openCommand"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DummyMemory">
		<xsd:complexContent>
			<xsd:extension base="DeviceBase">
				<xsd:sequence>
					<xsd:element ref="Dimension"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
